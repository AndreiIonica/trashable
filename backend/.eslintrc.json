{
	"env": {
		"commonjs": true,
		"es6": true,
		"node": true,
		"mocha": true
	},
	"extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "error",
		"no-unused-vars": "warn",
		"linebreak-style": "off",
		"camelcase": 0,
		"node/no-unpublished-require": "off",
		"no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"]
	}
}
